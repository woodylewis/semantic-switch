{
  "name": "semantic-switch",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "latest",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "react": "latest",
    "react-dom": "latest",
    "react-router-dom": "latest",
    "react-scripts": "latest",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-router-dom": "latest",
    "prettier": "^2.8.2",
    "ts-node": "^10.9.1",
    "typescript": "latest"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "clean": "rm -rf node_modules",
    "eject": "react-scripts eject",
    "s3": "cd build && yarn build && aws --profile wls s3 sync ./ s3://semanticswitch.network",
    "ci": "aws --profile wls cloudfront create-invalidation --distribution-id E16S2N8A6CUCF4 --paths '/*'",
    "gi": "aws --profile wls cloudfront get-invalidation --distribution-id E16S2N8A6CUCF4 --id ",
    "prettify": "prettier --write 'src/**/*.tsx'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
